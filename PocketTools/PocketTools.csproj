<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <SignAssembly>false</SignAssembly>
    <Version>0.8.0.0</Version>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageTags>utilities</PackageTags>
    <Company />
    <Copyright />
    <Description>General-purpose static and extension methods</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageIcon>PocketTools.png</PackageIcon>
    <RepositoryType>git</RepositoryType>
    <PackageProjectUrl>https://github.com/Gaslite/PocketTools</PackageProjectUrl>
  </PropertyGroup>

  <ItemGroup>
    <None Include="PocketTools.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
